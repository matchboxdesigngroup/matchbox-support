{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "matchbox/userback-toggle",
	"version": "1.0.0",
	"title": "Userback Toggle",
	"category": "design",
	"description": "A toggle switch to enable or disable Userback feedback modal in the editor.",
	"example": {},
	"supports": {
		"html": false,
		"color": {
			"background": true,
			"text": true
		},
		"layout": {
			"allowInheriting": true
		},
		"spacing": {
			"blockGap": true,
			"padding": true
		},
		"inserter": false
	},
	"attributes": {
		"style": {
			"type": "object"
		}
	},
	"textdomain": "matchbox-support",
	"editorScript": "file:./index.js"
}
